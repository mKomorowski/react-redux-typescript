{
  "name": "react-redux-typescript",
  "version": "0.2.0",
  "description": "Typescript boilerplate for React/Redux development with webpack, testing and linting included",
  "keywords": [
    "typescript",
    "react",
    "redux",
    "react router",
    "react hooks",
    "react testing library",
    "webpack",
    "eslint",
    "stylelint",
    "sass",
    "babel"
  ],
  "author": "Michal Komorowski",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mkomorowski/react-redux-typescript.git"
  },
  "scripts": {
    "lint:css": "stylelint \"src/**/*.scss\"",
    "lint:ts": "eslint \"src/**/*.{ts,tsx}\"",
    "lint:ts:fix": "eslint --fix \"src/**/*.{ts,tsx}\"",
    "lint": "npm run lint:css && npm run lint:ts",
    "test": "jest --config config/jest/config.js",
    "check-types": "tsc",
    "start": "webpack-dev-server --config=config/webpack/dev.js"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.0",
    "@babel/plugin-transform-runtime": "^7.16.0",
    "@babel/preset-env": "^7.16.0",
    "@babel/preset-react": "^7.16.0",
    "@babel/preset-typescript": "^7.16.0",
    "@testing-library/jest-dom": "^5.15.0",
    "@testing-library/react": "^12.1.2",
    "@types/classnames": "^2.3.1",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.9.2",
    "@types/react": "^17.0.11",
    "@types/react-dom": "^17.0.8",
    "@types/react-redux": "^7.1.16",
    "@types/react-router-dom": "^5.1.7",
    "@types/redux-thunk": "^2.1.0",
    "@types/reselect": "^2.2.0",
    "@types/testing-library__jest-dom": "^5.14.0",
    "@typescript-eslint/eslint-plugin": "^4.27.0",
    "@typescript-eslint/parser": "^4.27.0",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.2.3",
    "copy-webpack-plugin": "^9.1.0",
    "css-loader": "^6.5.1",
    "eslint": "^8.2.0",
    "eslint-config-airbnb": "^19.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jest-dom": "^3.9.2",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.27.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "image-webpack-loader": "^8.0.1",
    "jest": "^27.3.1",
    "node-sass": "^6.0.1",
    "postcss": "^8.3.11",
    "postcss-loader": "^6.2.0",
    "prettier": "^2.4.1",
    "react-hot-loader": "^4.13.0",
    "sass-loader": "^12.3.0",
    "style-loader": "^3.3.1",
    "stylelint": "^14.0.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard-scss": "^2.0.1",
    "stylelint-custom-processor-loader": "^0.6.0",
    "stylelint-prettier": "^2.0.0",
    "ts-jest": "^27.0.7",
    "typescript": "^4.4.4",
    "webpack": "^5.64.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-middleware": "^5.2.1",
    "webpack-dev-server": "^4.4.0",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "classnames": "^2.3.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.4",
    "react-router-dom": "^5.2.0",
    "redux": "^4.1.0",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0"
