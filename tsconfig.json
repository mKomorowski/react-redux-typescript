{
  "compilerOptions": {
    "outDir": "./dist/",
    "target": "es6",
    "module": "commonjs",
    "moduleResolution": "node",
    "noResolve": false,
    "noImplicitAny": false,
    "removeComments": true,
    "sourceMap": true,
    "esModuleInterop": true,
    "allowJs": true,
    "jsx": "react",
    "types": ["node", "jest", "@testing-library/jest-dom"]
  },
  "include": ["src", "types"],
  "exclude": ["node_modules"]
}
